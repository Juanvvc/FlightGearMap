<?xml version="1.0"?>

<PropertyList>

 <generic>

  <output>
   <line_separator>newline</line_separator>
   <var_separator>:</var_separator>

   <chunk>
    <name>speed</name>
    <format>%f</format>
    <type>float</type>
    <node>/instrumentation/airspeed-indicator/indicated-speed-kt</node>
   </chunk>

   <chunk>
    <name>Engine 0 rpm</name>
    <format>%d</format>
    <node>/engines/engine[0]/rpm</node>
   </chunk>

   <chunk>
    <name>heading</name>
    <format>%03d</format>
    <node>/orientation/heading-deg</node>
   </chunk>

   <chunk>
    <name>altitude</name>
    <format>%f</format>
    <type>float</type>
    <node>/instrumentation/altimeter/indicated-altitude-ft</node>
   </chunk>

   <chunk>
    <name>rate of climb</name>
    <format>%f</format>
    <type>float</type>
    <node>/instrumentation/vertical-speed-indicator/indicated-speed-fpm</node>
   </chunk>

   <chunk>
    <name>pitch angle</name>
    <type>float</type>
    <format>%f</format>
    <node>/instrumentation/attitude-indicator/indicated-pitch-deg</node>
   </chunk>

   <chunk>
    <name>Roll angle</name>
    <type>float</type>
    <format>%f</format>
    <node>/instrumentation/attitude-indicator/indicated-roll-deg</node>
   </chunk>

   <chunk>
    <name>latitude</name>
    <type>float</type>
    <format>%.10f</format>
    <node>/position/latitude-deg</node>
   </chunk>

   <chunk>
    <name>longitude</name>
    <type>float</type>
    <format>%.10f</format>
    <node>/position/longitude-deg</node>
   </chunk>

   <chunk>
    <name>time (seconds from midnight, GMT)</name>
    <format>%d</format>
    <node>/instrumentation/clock/indicated-sec</node>
   </chunk>

   <chunk>
    <name>Turn rate</name>
    <type>float</type>
    <format>%f</format>
    <node>/instrumentation/turn-indicator/indicated-turn-rate</node>
   </chunk>

   <chunk>
    <name>Slip skid</name>
    <type>float</type>
    <format>%f</format>
    <node>/instrumentation/slip-skid-ball/indicated-slip-skid</node>
   </chunk>

   <chunk>
	   <name>Heading indicator</name>
	   <type>float</type>
	   <format>%f</format>
	   <node>/instrumentation/heading-indicator/indicated-heading-deg</node>
   </chunk>

   <chunk>
	   <name>Fuel 1</name>
	   <type>float</type>
	   <format>%f</format>
	   <node>/consumables/fuel/tank/level-gal_us</node>
   </chunk>

   <chunk>
	   <name>Fuel 2</name>
	   <type>float</type>
	   <format>%f</format>
	   <node>/consumables/fuel/tank[1]/level-gal_us</node>
   </chunk>

   <chunk>
	   <name>Oil Press</name>
	   <type>float</type>
	   <format>%f</format>
	   <node>/engines/engine/oil-pressure-psi</node>
   </chunk>

   <chunk>
	   <name>Oil Temp F</name>
	   <type>float</type>
	   <format>%f</format>
	   <node>/engines/engine/oil-temperature-degf</node>
   </chunk>

   <chunk>
	   <name>Amp</name>
	   <type>float</type>
	   <format>%f</format>
	   <node>/systems/electrical/amps</node>
   </chunk>

   <chunk>
	   <name>Volts</name>
	   <type>float</type>
	   <format>%f</format>
	   <node>/systems/electrical/volts</node>
   </chunk>

   <chunk>
	   <name>Nav1 to flag </name>
	   <type>bool</type>
	   <node>/instrumentation/nav/to-flag</node>
   </chunk>
 
   <chunk>
	   <name>Nav1 from flag </name>
	   <type>bool</type>
	   <node>/instrumentation/nav/from-flag</node>
   </chunk>

   <chunk>
	   <name>Nav1 heading deflection </name>
	   <type>float</type>
	   <format>%f</format>
	   <node>/instrumentation/nav/heading-needle-deflection</node>
   </chunk>

   <chunk>
	   <name>Nav2 to flag </name>
	   <type>bool</type>
	   <node>/instrumentation/nav[1]/to-flag</node>
   </chunk>
 
   <chunk>
	   <name>Nav2 from flag </name>
	   <type>bool</type>
	   <node>/instrumentation/nav[1]/from-flag</node>
   </chunk>

   <chunk>
	   <name>Nav2 heading deflection </name>
	   <type>float</type>
	   <format>%f</format>
	   <node>/instrumentation/nav[1]/heading-needle-deflection</node>
   </chunk>
 
 </output>

 </generic>

</PropertyList>
